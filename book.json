{
    "title": "ScarSu's Gitbook",
    "author": "ScarSu",
    "description": "Outputing is Learning",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles": {
        "website": "./src/css/website.css"
    },
    "structure": {
        "readme": "README.md"
    },
    "plugins": [
        "-sharing",
        "splitter",
        "expandable-chapters-small",
        "anchors",
        "github",
        "github-buttons",
        "sharing-plus",
        "anchor-navigation-ex",
        "favicon"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/scarsu"
        },
        "github-buttons": {
            "buttons": [{
                "user": "scarsu",
                "repo": "myGitbook",
                "type": "star",
                "size": "small",
                "count": true
            }]
        },
        "donate": {
            "alipay": "./src/images/donate.png",
            "title": "",
            "button": "赞赏",
            "alipayText": " "
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": false,
            "whatsapp": false,
            "all": [
                "google", "facebook", "weibo", "twitter",
                "qq", "qzone", "linkedin", "pocket"
            ]
        },
        "anchor-navigation-ex": {
            "showLevel": false
        },
        "favicon": {
            "shortcut": "./src/images/favicon.jpg",
            "bookmark": "./src/images/favicon.jpg",
            "appleTouch": "./src/images/apple-touch-icon.jpg",
            "appleTouchMore": {
                "120x120": "./src/images/apple-touch-icon.jpg",
                "180x180": "./src/images/apple-touch-icon.jpg"
            }
        }
    }
}